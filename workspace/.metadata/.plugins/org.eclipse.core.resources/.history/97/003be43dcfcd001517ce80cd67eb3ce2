//Use depth-first search, check if a tree contains a value
public class Problem17 {
	static Boolean Solution(TreeNode head, int value){
		if(head.val==value){
			return true;
		}
		
		if(head.right!=null){
			Solution(head.right,value);
		}
		return false;
	}
	
	public static void main(String[] args){
		
	}
}
