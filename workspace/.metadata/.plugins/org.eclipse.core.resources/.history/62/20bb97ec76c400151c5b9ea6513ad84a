
public class Solution {
	public static ListNode oddEvenList(ListNode head){
		ListNode temp = head;
		if(head.val%2==head.next.val%2){
			head=head.next;
		}else{
			int switchNo=head.val;
			head.val=temp.val;
			temp.val=switchNo;
			temp=temp.next;
			head=head.next;
		}
		return null;
	}
	public static void main(String[] args){
		int[] test={1,2,3,4,5,6,7,8,9};
		ListNode testHead = new ListNode(test[0]);
		ListNode start=testHead;
		for(int i=1; i<test.length; i++){
			testHead.next=new ListNode(test[i]);
			testHead=testHead.next;
		}
		
		oddEvenList(start);
	}

}
