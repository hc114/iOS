
public class Solution {
	static ListNode createList(char[] chars){
		ListNode head = null;
		for(int i=0; i<chars.length; i++){
			if(head==null){
				head=new ListNode(chars[i]);
				head.prev=null;
			}else{
				ListNode temp = head;
				head=head.next;
				head=new ListNode(chars[i]);
				head.prev=temp;
			}
		}
		return head;
	}
	static void LIFO(ListNode head){
		while(head!=null){
			char s=head.val;
			System.out.println(head.val);
			head=head.prev;
		}
	}
	public static void main(String[] args){
		//Create linked list
		char[] c = {'a','b','c','d','e','f','g'};
		LIFO(createList(c));
	}
}
