public class Solution {

	public static boolean canWinNim(int n) {
		//No matter what size the heap is we want to be at 8 for our turn.

		//Take away maximum number until the number of pebbles in the heap reach 8<=n<11

		//Strategy: Try to have the opponent reach 4. So if it is 8 other person's turn 
		int count=0;
		while(n>11){
			count++;
			n=n-3;
			System.out.println(n);
		}
		//Is it your turn or my turn
		if(count % 2==0){
			System.out.println("it's my turn");
		}else{
			System.out.println("it's your turn");
		}
		
		return false;
	}

	public static void main(String args[]){
		canWinNim(199);
		return;
	}
};