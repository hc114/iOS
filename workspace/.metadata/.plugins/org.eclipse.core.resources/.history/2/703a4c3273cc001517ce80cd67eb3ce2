
public class Problem4 {
	static void Solution(){
		
	}
	
	static void main(String[] args){
		int[][] input = new int[4][4];
		int[] a = {21,262,211,216,4,3,5,6};
		
		int count = 0;
		int column=0;
		for(int i=0; i<a.length; i++){
			input[column][i]=a[i];
			count++;
			if(count==4){
				column++;
				count=0;
			}
		}
		System.out.println(input);
	}

}
