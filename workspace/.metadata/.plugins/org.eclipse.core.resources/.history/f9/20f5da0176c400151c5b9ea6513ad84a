
public class Solution {
	public ListNode oddEvenList(ListNode head){
		ListNode temp = head;
		if(head.val%2==head.next.val%2){
			head=head.next;
		}else{
			int switchNo=head.val;
			head.val=temp.val;
			temp.val=switchNo;
			temp=temp.next;
		}
		return null;
	}

	public static void main(String[] args){
		
	}
}
