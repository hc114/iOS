
public class Solution {
	public static ListNode oddEvenList(ListNode head){
		return head;
	}
	
	public static void main(String[] args){
		int[] test={1,3,5,7,9,2,4,6,8};
		ListNode testHead = new ListNode(test[0]);
		ListNode start=testHead;
		for(int i=1; i<test.length; i++){
			testHead.next=new ListNode(test[i]);
			testHead=testHead.next;
		}
		//creating the output List
		for(int j=1; j<test.length; j++){
			ListNode outputHead=oddEvenList(start);
			System.out.println(outputHead.val);
			outputHead=outputHead.next;
		}
	}

}
