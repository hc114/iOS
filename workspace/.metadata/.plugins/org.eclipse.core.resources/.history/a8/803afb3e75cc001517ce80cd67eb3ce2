
public class Problem4 {
	static void Solution(){
		
	}
	
	public static void main(String[] args){
		int[][] input = new int[2][4];
		int[] a = {211,262,211,216,4,3,5,6};
		
		int count = 0;
		int column=0;
		int index=0;
		for(int i=0; i<2; i++){
			for(int j=0; j<4; j++){
				input[i][j]=a[index];
				index++;
			}
		}
		System.out.println(input);
	}

}
